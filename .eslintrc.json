{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "max-len": [
            2,
            120
        ],
        "no-trailing-spaces": [
            2,
            { "skipBlankLines": true }
        ]
    }
}
